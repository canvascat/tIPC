{
  "name": "@tipc/electron",
  "version": "0.0.1",
  "description": "A simple and powerful IPC library for Electron",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./main": {
      "import": "./dist/main.mjs",
      "require": "./dist/main.js",
      "types": "./dist/main.d.ts"
    },
    "./renderer": {
      "import": "./dist/renderer.mjs",
      "require": "./dist/renderer.js",
      "types": "./dist/renderer.d.ts"
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "typescript",
    "electron",
    "ipc",
    "trpc"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^37.2.0",
    "rxjs": "^7.8.2",
    "tsdown": "^0.12.8",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "peerDependencies": {
    "electron": "^37.2.0",
    "rxjs": "^7.8.2"
  },
  "publishConfig": {
    "access": "public"
  }
}